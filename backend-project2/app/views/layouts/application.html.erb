<!DOCTYPE html>
<html>
  <head>
    <title>BackendProject2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>


  <body>
    <% if @current_user.present? %>
    <nav>
    <%= link_to 'Logout', login_path, method: 'delete', class: "logout"  %>
    <%= link_to 'Profile', users_path, class: "profile"  %> </nav>
    <p class ="welcomeB">Welcome back, <%= @current_user %> </p>
    <hr>
    <% else %>
    <%= link_to 'Signup', new_user_path, class: "signup" %>

    <% end %>
    <%= yield %>

  </body>
</html>
